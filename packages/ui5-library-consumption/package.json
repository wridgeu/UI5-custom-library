{
  "name": "ui5-library-consumption",
  "version": "1.0.0",
  "private": true,
  "description": "UI5 application to demonstrate the consumption of a custom library",
  "scripts": {
    "start": "ui5 serve -o index.html",
    "build": "ui5 build self-contained --all --dest ./dist",
    "start:build": "ui5 serve -o index.html --config ui5-dist.yaml", 
    "lint": "eslint ."
  },
  "dependencies": {
    "ui5-library": "^1.0.0"
  },
  "devDependencies": {
    "@ui5/cli": "^2.8.0",
    "eslint": "^7.18.0",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.3",
    "ui5-middleware-livereload": "^0.5.1"
  },
  "ui5": {
    "dependencies": [
      "ui5-library",
      "ui5-middleware-livereload"
    ]
  }
}
