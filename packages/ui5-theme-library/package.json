{
  "name": "ui5-theme-library",
  "version": "1.0.0",
  "private": true,
  "description": "Custom Theme Library",
  "scripts": {
    "start": "ui5 serve -o",
    "_build": "yarn clean && ui5 build --config ui5-dist.yaml",
    "clean": "rimraf dist/resources/*",
    "watch": "npm-watch"
  },
  "watch": {
    "_build": {
      "patterns": [
        "src"
      ],
      "extensions": "less",
      "quiet": false
    }
  },
  "keywords": [
    "testing",
    "custom-theming",
    "learning",
    "theme-library",
    "ui5"
  ],
  "author": "Marco Beier",
  "license": "ISC",
  "devDependencies": {
    "npm-watch": "^0.7.0",
    "rimraf": "^3.0.2"
  },
  "dependencies": {}
}
